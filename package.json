{
  "name": "twitter-mini",
  "version": "1.0.0",
  "description": "Twitter mini for Twitter Desktop APP",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . \"Twitter Mini\" --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=dist/release-builds",
    "package-win": "electron-packager . \"Twitter Mini\" --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=dist/release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Twitter Mini\"",
    "package-linux": "electron-packager . \"Twitter Mini\" --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=dist/release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dongri/twitter-mini.git"
  },
  "keywords": [
    "twitter",
    "electron",
    "macos",
    "linux",
    "windows"
  ],
  "author": "Dongri Jin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dongri/twitter-mini/issues"
  },
  "homepage": "https://github.com/dongri/twitter-mini#readme",
  "devDependency": {},
  "dependencies": {
    "electron": "^5.0.0",
    "electron-packager": "^13.1.1",
    "electron-positioner": "=3.0.0"
  }
}
